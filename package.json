	"name": "sunbird",
	"version": "1.0.0",
	"description": "Framework for Sunbird",
	"scripts": {
		"profile": "webpack --profile --json > stats.json",
		"build": "ng-packagr -p package.json && cp -R scripts dist/scripts && cp dependencies.json dist/",
